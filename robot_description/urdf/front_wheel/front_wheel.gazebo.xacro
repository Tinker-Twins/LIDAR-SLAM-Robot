<?xml version="1.0"?>
<robot xmlns:xacro="http://ros.org/wiki/xacro">
  <xacro:macro name="front_wheel_gazebo_v0" params="prefix">
    <gazebo reference="${prefix}_wheel_link">
      <selfCollide>false</selfCollide>
      <mu1 value="0.8" />
      <mu2 value="0.8" />
      <kp  value="100000.0" />
      <kd  value="10.0" />
    </gazebo>
  </xacro:macro>
</robot>

